    <rdfs:label xml:lang="fr">P138 représente</rdfs:label>
    <rdfs:label xml:lang="pt">P138 representa</rdfs:label>
    <rdfs:label xml:lang="ru">P138 представляет</rdfs:label>
    <rdfs:label xml:lang="zh">P138 描绘</rdfs:label>
    <skos:notation>P138</skos:notation>
    <rdfs:domain rdf:resource="http://erlangen-crm.org/current/E36_Visual_Item"/>
